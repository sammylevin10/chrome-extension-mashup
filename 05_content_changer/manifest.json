{
  "manifest_version": 2,
  "name": "Bodies Inc. for Instagram",
  "version": "1.0",
  "description": "This extensions alters the look and content of an Instagram page.",
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*",
        "http://www.instagram.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
      "images/rainbow.png"
  ]
}
